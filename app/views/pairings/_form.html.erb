<%= form_for @pairing  do |f| %>

  <%=shows_error_messages(@pairing)%><br>

  <%= f.label :Wine %>
  <%= f.collection_select :wine_id, Wine.all,:id,:wine_name%><br>

  <%= f.label :Cookie %>
  <%= f.collection_select :cookie_id, Cookie.all,:id,:cookie_name%><br>

  <%= f.submit :class => 'btn btn-primary'%>
<% end %>
