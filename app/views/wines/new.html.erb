<h2> Add a new wine<h2>

  <%= form_for @wine  do |f| %>
    <%= f.label :name %>
    <%= f.text_field :wine_name %>
  <br>

    <%= f.label :grape_varietal %>
    <%= f.text_field :grape_varietal %>
  <br>
    <%= f.label :origin %>
    <%= f.text_field :origin %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
<br>
    <%= f.submit %>
  <% end %>

<%= link_to 'Home', home_url%>
